{
  "name": "database",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "graphql": "^14.4.2",
    "lerna": "^3.15.0",
    "prisma": "^1.34.1",
    "prisma-client-lib": "^1.34.1",
    "quicktype": "^15.0.197"
  },
  "scripts": {
    "start": "docker-compose up -d && yarn deploy:local",
    "start:force": "docker-compose up -d --force-recreate ",
    "deploy": "prisma deploy --e .env.production",
    "deploy:staging": "prisma deploy -e .env.staging",
    "deploy:local": "yarn wait && prisma deploy",
    "deploy:local:force": "yarn wait && prisma deploy --force",
    "seed": "babel-node src/seed",
    "wait": "./src/wait.sh"
  },
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "faker": "^4.1.0"
  },
  "babel": {
    "extends": "../../.babelrc"
  }
}
